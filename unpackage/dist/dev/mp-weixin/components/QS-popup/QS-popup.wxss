

/* fadeInMiddle */
.QS_fadeInMiddle.data-v-aae4d9b8 {
	position: fixed;
	top: 50%;
	left: 50%;
	-webkit-transform: translate(-50%, -50%);
	        transform: translate(-50%, -50%);
	pointer-events: none;
	opacity: 0;
	-webkit-transition: all .3s;
	transition: all .3s;
}
.QS_fadeInMiddle.show.data-v-aae4d9b8{
	-webkit-transform: translate(-50%, -50%);
	        transform: translate(-50%, -50%);
	opacity: 1;
	pointer-events: auto;
	-webkit-transition-delay: .2s;
	        transition-delay: .2s;
}
.QS_fadeInMiddle.hide.data-v-aae4d9b8{
	-webkit-transition-delay: 0s;
	        transition-delay: 0s;
}
